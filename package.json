{
  "name": "webpack-jarvis",
  "version": "0.3.2",
  "description": "A very intelligent webpack dashboard",
  "main": "dist/server/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "prebuild": "rm -rf dist",
    "copy:server": "ncp src/server dist/server",
    "copy:assets": "ncp src/index.html dist/index.html && ncp src/assets dist/assets",
    "build": "webpack --node-env=production --config config/webpack && npm run copy:server && npm run copy:assets",
    "watch": "JARVIS_ENV=development node --trace-deprecation node_modules/webpack/bin/webpack.js --node-env=development --watch --config config/webpack"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zouhir/jarvis.git"
  },
  "license": "MIT",
  "author": {
    "email": "zouhir@zouhir.org",
    "name": "Zouhir",
    "url": "https://zouhir.org"
  },
  "peerDependencies": {
    "webpack": "*"
  },
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "import-from": "^4.0.0",
    "obj-str": "^1.0.0",
    "parse-authors": "^0.2.4",
    "polka": "^0.5.2",
    "react-favicon": "^1.0.1",
    "serve-static": "^1.13.1",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "all-contributors-cli": "^6.24.0",
    "autoprefixer": "^10.4.12",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^6.7.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "ncp": "^2.0.0",
    "postcss-loader": "^7.0.1",
    "preact": "^10.11.1",
    "preact-compat": "^3.16.0",
    "preact-habitat": "^3.3.0",
    "preact-markup": "^2.1.1",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^8.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.21.0"
  },
  "lint-staged": {
    "src/**/*.{js,json,scss}": [
      "prettier --write",
      "git add"
    ]
  }
}
